set terminal png size 800, 600
set output "mpi.png"
set key outside
set xlabel "nombre de processeurs"
set logscale x 2
set logscale y 2
set ylabel "accélération"
plot for [i=0:5] "mpi" u 3:(($1==i)?($2/$4):(1/0)) w lines title 'mpi' linecolor rgb "red",\
     for [i=0:5] "mpi2" u 3:(($1==i)?($2/$4):(1/0)) w lines title 'mpi2' linecolor rgb "green",\
     x
